generator client {
  provider = "prisma-client-js"
  // output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users{
  id      Int       @id @default(autoincrement())
  name    String 
  email   String    @unique
  doubts  Doubts[] 
}

model Doubts {
  id        Int      @id @default(autoincrement())
  user_id   Int      
  room      String   
  doubt     String?  
  upvotes   Int      @default(0)
  user      Users    @relation(fields: [user_id], references: [id])
}